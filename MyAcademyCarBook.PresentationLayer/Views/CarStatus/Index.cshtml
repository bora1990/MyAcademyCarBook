@model List<MyAcademyCarBook.EntityLayer.Concrete.CarStatus>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<br />
<br />

<h1>Araba Durum Listesi</h1>
<br />
<br />


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Durum</th>
        <th>Sil</th>
        <th>Güncelle</th>
       
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CarStatusID</td>
            <td>@item.CarStatusName</td>
            <td><a href="/CarStatus/RemoveCarStatus/@item.CarStatusID/" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/CarStatus/UpdateCarStatus/@item.CarStatusID/" class="btn btn-outline-success">Güncelle</a></td>
            
        </tr>
    }
</table>

<a href="/CarStatus/CreateCarStatus/" class="btn btn-outline-primary">Yeni Araba Durum Girişi</a>

